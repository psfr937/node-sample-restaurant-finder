{
  "name": "react-cool-starter",
  "version": "2.3.0",
  "description": "chillday app server",
  "main": "index.js",
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psfr937/chillday-app-server"
  },
  "keywords": [
    "redux",
    "express",
    "unit testing",
    "performance optimizing"
  ],
  "author": "Raymond Pang",
  "license": "Private",
  "bugs": {
    "url": "https://github.com/wellyshen/react-cool-starter/issues"
  },
  "homepage": "https://github.com/wellyshen/react-cool-starter",
  "scripts": {
    "start": "better-npm-run start",
    "start:dashboard": "webpack-dashboard yarn start",
    "start:production": "yarn build && yarn start:prod",
    "start:prod": "better-npm-run start:prod",
    "test": "better-npm-run test",
    "test:watch": "yarn test --watch",
    "clean:all": "yarn clean:build && yarn clean:test",
    "clean:test": "better-npm-run clean:test",
    "coveralls": "better-npm-run coveralls && yarn clean:test"
  },
  "betterScripts": {
    "start": {
      "command": "nodemon --inspect=0.0.0.0:9229 ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "NODE_HOST": "localhost",
        "LISTEN_TO": "localhost",
        "PORT": 3000
      }
    },
    "start:prod": {
      "command": "pm2 start ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "test": {
      "command": "jest --coverage",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "clean:build": {
      "command": "rimraf ./public/assets"
    },
    "clean:test": {
      "command": "rimraf ./coverage"
    },
    "coveralls": {
      "command": "cat ./coverage/lcov.info | coveralls"
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "useBuiltIns": true
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "loadable-components/babel",
      "dynamic-import-node",
      "lodash"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11",
    "safari >= 7"
  ],
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/tools/jest/setup.js"
    ],
    "globals": {
      "__DEV__": true
    }
  },
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "chalk": "2.3.1",
    "classnames": "^2.2.5",
    "compression": "1.7.2",
    "date-fns": "^1.30.1",
    "dot-env": "^0.0.1",
    "dotenv": "^6.0.0",
    "express": "4.17.3",
    "express-promise-router": "^3.0.2",
    "express-useragent": "^1.0.12",
    "fs": "^0.0.1-security",
    "hpp": "^0.2.2",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "imports-loader": "^0.8.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.1.1",
    "loadable-components": "1.1.1",
    "lodash": "4.17.5",
    "mailchimp-api-v3": "^1.12.0",
    "mkdirp": "^0.5.1",
    "mongojs": "^2.6.0",
    "morgan": "1.9.0",
    "multer": "^1.3.0",
    "node-bourbon": "^4.2.8",
    "nodemailer": "^4.6.7",
    "normalizr": "^3.2.4",
    "object-assign": "^4.1.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-facebook-token": "^3.3.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-jwt": "^3.0.1",
    "pg": "^7.4.1",
    "pm2": "^3.1.3",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-dom": "16.7.0",
    "redis-connection": "^5.4.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "scriptjs": "^2.5.9",
    "sequelize": "^4.37.6",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "2.4.5",
    "socket.io": "^2.1.1",
    "superagent": "^3.8.3",
    "uuid": "^3.2.1",
    "xoauth2": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "22.4.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-remove-console": "6.9.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^7.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "coveralls": "3.0.0",
    "cross-spawn": "6.0.4",
    "extracted-loader": "^1.0.6",
    "file-loader": "1.1.7",
    "jest": "22.4.0",
    "nock": "9.1.6",
    "nodemon": "1.15.1",
    "opn": "^5.2.0",
    "raf": "3.4.0",
    "rimraf": "^2.6.2"
  }
}
